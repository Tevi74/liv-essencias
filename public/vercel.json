{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/cities/(.*)", "destination": "/cities/$1/index.html" },
    { "source": "/liv-kamp", "destination": "/liv-kamp/index.html" },
    { "source": "/liv-kamp/", "destination": "/liv-kamp/index.html" }
  ]
}
